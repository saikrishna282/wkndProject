<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <allow
        jcr:primaryType="rep:GrantACE"
        rep:principalName="sling-context-aware-config-web-console"
        rep:privileges="{Name}[jcr:read]"/>
    <allow1
        jcr:primaryType="rep:GrantACE"
        rep:principalName="sling-mapping"
        rep:privileges="{Name}[jcr:read]"/>
    <allow2
        jcr:primaryType="rep:GrantACE"
        rep:principalName="sling-readall"
        rep:privileges="{Name}[jcr:read]"/>
    <deny3
        jcr:primaryType="rep:DenyACE"
        rep:principalName="everyone"
        rep:privileges="{Name}[jcr:all]"/>
    <allow4
        jcr:primaryType="rep:GrantACE"
        rep:principalName="compat-codeupgrade-service"
        rep:privileges="{Name}[jcr:modifyAccessControl,jcr:read,rep:write]"/>
    <allow5
        jcr:primaryType="rep:GrantACE"
        rep:principalName="dam-users"
        rep:privileges="{Name}[jcr:read]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="/*/dam/*"/>
    </allow5>
    <allow6
        jcr:primaryType="rep:GrantACE"
        rep:principalName="assetusagetrackeruser"
        rep:privileges="{Name}[jcr:read]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="/*/dam/*"/>
    </allow6>
    <allow7
        jcr:primaryType="rep:GrantACE"
        rep:principalName="forms-users"
        rep:privileges="{Name}[jcr:read]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="/*/dam/*"/>
    </allow7>
    <allow8
        jcr:primaryType="rep:GrantACE"
        rep:principalName="repository-change-listener-service"
        rep:privileges="{Name}[jcr:read]"/>
</jcr:root>
